curl "https://api.groq.com/openai/v1/chat/completions" \
  -X POST \
  -H "Content-Type: application/json" \
  -H "Authorization: Bearer ${GROQ_API_KEY}" \
  -d '{
         "messages": [
           {
             "role": "user",
             "content": ""
           }
         ],
         "model": "llama-3.3-70b-versatile",
         "temperature": 1,
         "max_tokens": 32768,
         "top_p": 1,
         "stream": true,
         "stop": null
       }'
  
curl "https://api.groq.com/openai/v1/chat/completions" \
